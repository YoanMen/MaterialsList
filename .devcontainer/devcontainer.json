{
  "name": "HardwaresList environment",
  "dockerComposeFile": "compose.yaml",
  "service": "dev",
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig",
        "aaron-bond.better-comments",
        "DEVSENSE.phptools-vscode",
        "bmewburn.vscode-intelephense-client",
        "xdebug.php-debug",
        "DEVSENSE.profiler-php-vscode",
        "kokororin.vscode-phpfmt",
        "streetsidesoftware.code-spell-checker",
        "streetsidesoftware.code-spell-checker-french",
        "oderwat.indent-rainbow",
        "usernamehw.errorlens",
        "SirTori.indenticator",
        "xabikos.JavaScriptSnippets",
        "ms-vscode.makefile-tools",
        "christian-kohler.path-intellisense",
        "cvergne.vscode-php-getters-setters-cv",
        "rexshi.phpdoc-comment-vscode-plugin",
        "bradlc.vscode-tailwindcss",
        "Zarifprogrammer.tailwind-snippets",
        "junstyle.twig-language",
        "mblode.twig-language-2",
        "moetelo.twiggy",
        "redhat.vscode-yaml",
        "xdebug.php-pack",
        "zobo.php-intellisense",
        "MehediDracula.php-namespace-resolver",
        "kokororin.vscode-phpfmt",
        "MrChetan.phpstorm-parameter-hints-in-vscode",
        "esbenp.prettier-vscode",
        "SonarSource.sonarlint-vscode",
        "liamhammett.temphpest"
      ]
    }
  },
  "forwardPorts": ["mailer:8026", "phpmyadmin:8102"]
}
